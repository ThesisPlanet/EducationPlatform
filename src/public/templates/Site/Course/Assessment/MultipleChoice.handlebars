<form class="form">
	<h2>{{title}}</h2>
	<div class="answers">
		<div class="answer">
			{{#each possibleAnswers}}
			<input type="checkbox" name="answer_{{@index}}_correct" />{{this}}<br /> 
			{{/each}}
		</div>
	</div>
	<input type="hidden" name="question_id" value="{{question_id}}"/>
	<input type="button" class="btn btn-primary" name="submit_answers" value="Next" />
</form>